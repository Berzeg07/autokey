.banner                
    .container
        .banner__inner
            
            .chief-about
                .chief-about__img
                    img(src="img/chief.png")
                .chief-about__article
                    span Александр 
                    |руководитель компании AutoKey
                
                
            form.search-master(action="#")
                .search-master__title 
                    span Сделаем ключ для автомобиля за 1 час
                    br
                    b с выездом в Москве по цене на 
                        i 40% дешевле дилера
                    
                .payment                                      
                    .payment-inner
                        .payment-item
                            .select-custom
                                select#selectBroken
                                    option(value="Заголовок") Выберите услугу
                                    option(value="/remont-zamka-zaziganiya/") Ремонт замка зажигания
                                    option(value="/zamena-zamka-zaziganiya/") Замена замка зажигания
                        .payment-item
                            .select-custom
                                select#selectMark
                                    option(value="Заголовок") Марка автомобиля
                                    option(value="Audi") Audi
                                    option(value="BMW") BMW
                        .payment-item
                            .select-custom
                                select#selectModel
                                    option(value="Заголовок") Модель автомобиля
                                    
                        .payment-item
                            button(type="button").payment__btn#urlBtn 
                                span Рассчитать стоимость


